{
  "name": "@salocreative/react-ui",
  "version": "1.7.7",
  "description": "Simple set of React UI components.",
  "main": "./dist/modules/index.js",
  "scripts": {
    "bundle-demo": "webpack --config webpack.config.demo.js",
    "bundle-dist": "webpack --config webpack.config.dist.js",
    "bundle-modules": "rm -rf dist/modules/* && babel src --loose --out-dir dist/modules",
    "bundle": "npm run bundle-demo && npm run bundle-dist && npm run bundle-modules",
    "demo": "webpack-dev-server --config webpack.config.demo.js",
    "lint": "eslint ./src"
  },
  "author": "Rich Comber",
  "repository": {
    "type": "git",
    "url": "https://github.com/SaloCreative/react-ui.git"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^0.14.2",
    "react-fontawesome": "^1.6.1",
    "react-router": "3"
  },
  "devDependencies": {
    "babel": "5.8.29",
    "babel-core": "5.8.30",
    "babel-loader": "5.3.2",
    "classnames": "2.1.5",
    "eslint": "1.6.0",
    "mocha": "2.3.3",
    "react-addons-test-utils": "0.14.2",
    "react-dom": "0.14.2",
    "sinon": "1.17.1",
    "trash": "3.1.2",
    "unexpected": "10.0.2",
    "unexpected-sinon": "8.0.0",
    "webpack": "1.12.1",
    "webpack-dev-server": "1.10.1"
  }
}
